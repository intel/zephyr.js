{
    "module": "sensor_arc",
    "targets": ["arc"],
    "depends": ["ipm_arc"],
    "zephyr_conf": {
        "arc": [
            "CONFIG_SENSOR=y",
            "CONFIG_GPIO=y",
            "CONFIG_SPI=y",
            "CONFIG_SPI_0=n",
            "CONFIG_SPI_1=n",
            "CONFIG_BMI160=y"
        ]
    },
    "zjs_config": ["-DBUILD_MODULE_SENSOR"]
}
