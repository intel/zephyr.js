{
    "module": "uart",
    "require": "uart",
    "depends": ["buffer", "events"],
    "zephyr_conf": {
        "all": ["CONFIG_UART_INTERRUPT_DRIVEN=y"],
        "arduino_101": [
            "CONFIG_GPIO=y",
            "CONFIG_USB=y",
            "CONFIG_USB_DW=y",
            "CONFIG_USB_DEVICE_STACK=y",
            "CONFIG_USB_CDC_ACM=y",
            "CONFIG_SERIAL=y",
            "CONFIG_UART_LINE_CTRL=y"
        ],
        "ashell": ["CONFIG_USB_CDC_ACM=y"]
    },
    "zjs_config": ["-DBUILD_MODULE_UART"],
    "src": ["src/zjs_uart.c"]
}
